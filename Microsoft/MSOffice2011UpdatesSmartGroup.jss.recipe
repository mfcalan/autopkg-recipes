<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Updates the JSS Smart Group, no policy is created.</string>
        <key>Identifier</key>
        <string>com.github.mfcalan.jss.Office2011UpdatesSmartGroup</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Microsoft Office 2011</string>
            <key>CATEGORY</key>
            <string>APPLICATIONS</string>
            <key>GROUP_NAME</key>
            <string>Autopkg: %NAME% - Needs update</string>
            <key>GROUP_TEMPLATE</key>
            <string>%RECIPE_DIR%/MSOffice2011UpdatesSmartGroup.xml</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.4.0</string>
        <key>ParentRecipe</key>
        <string>com.github.autopkg.pkg.Office2011Updates</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Processor</key>
                <string>MSOffice2011Versioner</string>
            </dict>
            <dict>
                <key>Processor</key>
                <string>JSSImporter</string>
                <key>Arguments</key>
                <dict>
                    <key>prod_name</key>
                    <string>%NAME%</string>
                    <key>category</key>
                    <string>%CATEGORY%</string>
                    <key>groups</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>%GROUP_NAME% to %version%</string>
                            <key>smart</key>
                            <true/>
                            <key>template_path</key>
                            <string>%GROUP_TEMPLATE%</string>
                        </dict>
                    </array>
                    <key>extension_attributes</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>MSOffice2011OutOfDate</string>
                            <key>ext_attribute_path</key>
                            <string>%RECIPE_DIR%/MSOffice2011ExtensionAttribute.xml</string>
                        </dict>
                    </array>
                </dict>
            </dict>
        </array>
    </dict>
</plist>
