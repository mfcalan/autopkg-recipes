<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Updates the Adobe Flash Player smart group, no policy is created, a second recipe creates the Self Service policy</string>
        <key>Identifier</key>
        <string>com.github.mfcalan.jss.AdobeFlashPlayerSmartGroup</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Adobe Flash Player</string>
            <key>CATEGORY</key>
            <string>PLUGINS-DRIVERS</string>
            <key>GROUP_NAME</key>
            <string>Autopkg: %NAME% - Needs update</string>
            <key>SECOND_GROUP_NAME</key>
            <string>Autopkg: %NAME% - Autoupdate</string>
            <key>GROUP_TEMPLATE</key>
            <string>%RECIPE_DIR%/AdobeFlashSmartGroup.xml</string>
            <key>GROUP_TEMPLATE2</key>
            <string>%RECIPE_DIR%/AdobeFlashSmartGroupAutoUpdate.xml</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.4.0</string>
        <key>ParentRecipe</key>
        <string>com.github.autopkg.pkg.FlashPlayerExtractPackage</string>
        <key>Process</key>
        <array>
            <dict>
                <key>Arguments</key>
                <dict>
                    <key>prod_name</key>
                    <string>%NAME%</string>
                    <key>category</key>
                    <string>%CATEGORY%</string>
                    <key>groups</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>%GROUP_NAME%</string>
                            <key>smart</key>
                            <true/>
                            <key>template_path</key>
                            <string>%GROUP_TEMPLATE%</string>
                        </dict>
                        <dict>
                            <key>name</key>
                            <string>%SECOND_GROUP_NAME%</string>
                            <key>smart</key>
                            <true/>
                            <key>template_path</key>
                            <string>%GROUP_TEMPLATE2%</string>
                        </dict>
                    </array>
                    <key>extension_attributes</key>
                    <array>
                        <dict>
                            <key>name</key>
                            <string>AdobeFlashVersion</string>
                            <key>ext_attribute_path</key>
                            <string>%RECIPE_DIR%/AdobeFlashPlayerExtensionAttribute.xml</string>
                        </dict>
                    </array>
                </dict>
                <key>Processor</key>
                <string>JSSImporter</string>
            </dict>
        </array>
    </dict>
</plist>
